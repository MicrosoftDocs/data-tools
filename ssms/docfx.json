{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md",
          "**/*.yml"
        ],

        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "_themes/**",
          "**/docfx.json",
          "_repo.en-us/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices.md",
          "SECURITY.md"
        ]
      }
    ],

    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.gif",
          "**/*.jpg",
          "**/*.svg"
        ],
        "exclude": [
          "**/obj/**"
        ]
      }
    ],

    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
      "uhfHeaderId": "MSDocsHeader-DocsSQL",
      "breadcrumb_path": "~/breadcrumb/toc.yml",
      "feedback_system": "Standard"
    },

    "fileMetadata": {},
    "template": [],
    "dest": "ssms",
    "rules": {
      "docs-link-absolute": {
        "exclude": [
          "download-sql-server-management-studio-ssms.md"
        ]
      },

      "titleSuffix": {
        "**/*.md": "SQL Server Management Studio (SSMS)",
        "agent/**/*.md": "SQL Server Agent",
        "visual-db-tools/**/*.md": "Visual Database Tools"
      },

      "ms.update-cycle": {
        "agent/**/*.md": "1825-days",
        "quickstarts/**/*.md": "1825-days",
        "register-servers/**/*.md": "1825-days",
        "tutorials/**/*.md": "1825-days",
        "visual-db-tools/**/*.md": "1825-days"
      },
      "sensitive-language-ma": {
      "exclude": [
        "agent/**/*.md"
        ]
      },

      "sensitive-language-sl": {
      "exclude": []
      },

      "sensitive-language-wl": {
      "exclude": []
      },
      
      "sensitive-language-bl": {
      "exclude": []
        }
      }
    }
  }