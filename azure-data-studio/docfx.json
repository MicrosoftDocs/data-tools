{
  "build": {

    "content": [
      {
        "src": "./",
        "files": [
          "**/*.md",
          "**/*.yml",
          "breadcrumb/toc.yml"
        ],
        "exclude": [
          "**/includes/**",
          "**/mref/**",
          "**/obj/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE"
        ]
      }
    ],

    "resource": [
      {
        "src": "./",
        "files": [
          "**/*.svg",
          "**/*.png",
          "**/*.gif",
          "**/*.jpg"
        ],
        "exclude": [
          "**/obj/**"
        ]
      }
    ],

    "overwrite": [],
    "externalReference": [],
    "markdownEngineName": "markdig",

    "globalMetadata": {
      "layout": "Conceptual",
      "uhfHeaderId": "MSDocsHeader-DocsSQL",
      "contributors_to_exclude": [
        "hexiaokai",
        "openpublishingbuild",
        "sudeepku",
        "saldana",
        "iRaindrop"
      ],
      "searchScope": [
        "azure data studio"
      ],

      "toc_preview": true,
      "feedback_system": "None",
      "feedback_github_repo": "MicrosoftDocs/data-tools",
      "feedback_product_url": "https://feedback.azure.com/d365community/search/04fe6ee0-3b25-ec11-b6e6-000d3a4f0da0?q=azure+data+studio",
      "titleSuffix": "Azure Data Studio",
      "recommendations": true
    },

    "fileMetadata": {
      "fileMetadata": {
        "feedback_system": {
          "azure-data-studio/**/*.md": "GitHub"
        },
        "feedback_product_url": {
          "azure-data-studio/**/*.md": "https://github.com/microsoft/azuredatastudio/issues/",
        },

        "manager": {
        "**/*.md": "jroth"
      },

      "breadcrumb_path": {
        "**/*.md": "~/breadcrumb/toc.yml"
      },
      "layout": {
        "**/index.yml": "LandingPage"
      },

      "titleSuffix": {
        "azure-data-studio/**/*.md": "Azure Data Studio"
      },

    "template": [],
    "dest": "azure-data-studio"
  }
}
